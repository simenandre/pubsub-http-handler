{
  "name": "pubsub-http-handler",
  "version": "6.0.0",
  "description": "Handle Google PubSub requests with ease",
  "keywords": [
    "google",
    "app",
    "cloud",
    "cloud-run",
    "run",
    "pubsub",
    "handler",
    "fastify",
    "fastify-plugin",
    "cloud-functions"
  ],
  "repository": "github:simenandre/pubsub-http-handler",
  "license": "Apache-2.0",
  "author": "Simen A. W. Olsen <cobraz@cobraz.no>",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "generate:docs": "typedoc --readme none --out docs src",
    "lint": "eslint '**/*.ts' --fix",
    "prepare": "husky install",
    "test": "jest --coverage src",
    "format": "prettier --write '**/*.ts'",
    "format:check": "prettier --check '**/*.ts'"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "prettier": "@simenandre/prettier",
  "dependencies": {
    "@fastify/type-provider-typebox": "^3.4.0",
    "@sinclair/typebox": "^0.34.0",
    "@types/express": "^4.17.13",
    "fastify": "^5.7.0",
    "fastify-plugin": "^4.2.0",
    "pino": "^8.0.0",
    "pino-cloud-logging": "^1.0.3"
  },
  "devDependencies": {
    "@bjerk/eslint-config": "^5.4.0",
    "@simenandre/prettier": "^5.0.0",
    "@tsconfig/node16": "^1.0.3",
    "@types/jest": "^29.5.0",
    "@types/json-schema-faker": "^0.5.1",
    "@types/node": "^16",
    "eslint": "^8.38.0",
    "eslint-plugin-jest": "^27.2.3",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "json-schema-faker": "^0.5.0-rcv.32",
    "lint-staged": "^13.1.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.24.4",
    "typedoc-plugin-markdown": "^3.1.1",
    "typescript": "^5.0.0",
    "zod": "^3.18.0"
  },
  "engines": {
    "node": ">=16"
  }
}
